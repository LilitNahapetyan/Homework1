{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiSettings": {
    "ReqresBaseUrl": "https://reqres.in/api/users/",
    "JsonPlaceholderBaseUrl": "https://jsonplaceholder.typicode.com/posts/"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logFolder/Log-.txt",
          "rollingInterval": "Week"
        }
      }
    ]
  }
}



